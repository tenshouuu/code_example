{
    "name": "examica-frontend",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "lint": "eslint \"./src/**/*.@(t|j)s?(x)\" --quiet",
        "lint:fix": "eslint \"./src/**/*.@(t|j)s?(x)\" --fix --quiet",
        "lint:css": "stylelint './src/**/*.@(t|j)s?(x)'",
        "build": "yarn run prod-gen-schema && cross-env NODE_ENV=production webpack",
        "dev": "yarn run dev-gen-schema && cross-env NODE_ENV=development yarn run vendors-dev && webpack",
        "dev-serv": "yarn run dev-gen-schema && cross-env NODE_ENV=development webpack-dev-server --hot --watch-poll",
        "prod-serv": "yarn run prod-gen-schema && cross-env NODE_ENV=production webpack-dev-server --hot --watch-poll",
        "dev-gen-schema": "cross-env NODE_ENV=development node codegen.js",
        "prod-gen-schema": "cross-env NODE_ENV=production node codegen.js",
        "vendors-dev": "cross-env NODE_ENV=development webpack --config webpack.dll.config.js",
        "vendors": "cross-env NODE_ENV=production webpack --config webpack.dll.config.js"
    },
